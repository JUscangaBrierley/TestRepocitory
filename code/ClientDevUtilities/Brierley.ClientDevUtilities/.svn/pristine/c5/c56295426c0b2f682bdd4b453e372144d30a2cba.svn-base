using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

using Brierley.FrameWork.Common.Exceptions;

namespace Brierley.LoyaltyWare.LWIntegrationSvc.Exceptions
{
    [Serializable]
    public class LWResponseMarshallingException : LWIntegrationException
    {               
        public LWResponseMarshallingException() : base() { }
        public LWResponseMarshallingException(string errorMessage) : base(errorMessage) { }
        public LWResponseMarshallingException(string errorMessage, Exception ex) : base(errorMessage, ex) { }
    }
}
