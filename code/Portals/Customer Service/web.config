      <add name="LanguageDefList" enabled="true" timeToLive="60" />
      <add name="ChannelDefList" enabled="true" timeToLive="60" />
      <add name="ContactStatusList" enabled="true" timeToLive="60" />
      <!-- RFE referral cookie expiration, in minutes -->
      <add key="LWRfeIdentifierExpiration" value="43200"/>
    </regions>
