	<appSettings>
		<add key="LWHibernateProfiler" value="false" />
		<add key="LWHibernateProfilerCaptureFile" value="D:\LOGS\CSPortal-NHProf.nhprof"/>
		<add key="LWAuditLoggingEnabled" value="false" />
		<add key="LW_CacheExpirationInterval" value="3600" /> <!-- Cache expiration interval in seconds -->
		<add key="PortalName" value="AmericanEagleCSPortal"/>
		<add key="LWOrganization" value="AmericanEagle"/>
		<add key="LWEnvironment" value="DevelopmentBLine"/>
		<add key="LWConfig" value="D:\devroot\americaneagle\workspace\LWConfig"/>
		<add key="LWAssemblyPath" value="D:\devroot\americaneagle\workspace\ClientLibs"/>
		<add key="LWContentRootURL" value="http://localhost:3435/content"/>
		<add key="LWContentRootURL" value="||ContentRootUrl||"/>
		<add key="CacheManagerType" value="EntLibCache" />    <!-- AppFabricCache|EntLibCache -->
		<add key="MessageMapAssembly" value="AmericanEagle.SDK.dll"/>
		<add key="MessageMapClass" value="AmericanEagle.SDK.ErrorMappers.AEErrorMapper"/>
		<add key="DisableHibernateCache " value="true" />
