   <springLogging>
	    <add key="loggingLevel" value="All" />
  	</springLogging>
	<system.web>
		<customErrors mode="On" defaultRedirect="error.html"/>
		<sessionState timeout="31" />
	 <compilation debug="true" targetFramework="4.6.1" />
		<httpModules>
			<add name="UrlRewriter" type="Brierley.WebFrameWork.Portal.UrlRewriter, Brierley.WebFrameWork"/>
		</httpModules>
		<authentication mode="Forms">
			<forms loginUrl="~/Login.aspx" timeout="30" name="PortalAuth" />
		</authentication>
		<pages  enableEventValidation="false" controlRenderingCompatibilityVersion="4.0"/>
	</system.web>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true">
			<add name="UrlRewriter" type="Brierley.WebFrameWork.Portal.UrlRewriter"/>
		</modules>
		<validation validateIntegratedModeConfiguration="false"/>
	</system.webServer>
	<system.serviceModel>
		<serviceHostingEnvironment aspNetCompatibilityEnabled="true"/>
	</system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
