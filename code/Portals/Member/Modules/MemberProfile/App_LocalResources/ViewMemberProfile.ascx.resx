  <data name="HtmlBlock_CFMemberRegistration2.Text" xml:space="preserve">
    <value>&lt;h3&gt;Address&lt;/h3&gt;&lt;hr/&gt;</value>
  </data>
  <data name="HtmlBlock_CFMemberSummary.Text" xml:space="preserve">
    <value>&lt;h2 id="Title"&gt;Member Info&lt;/h2&gt;&lt;table style="width:100%;"&gt;
                    &lt;tr&gt;
                        &lt;td&gt;&lt;strong&gt;Name:&lt;/strong&gt; ##member.FirstName##&amp;nbsp;##member.LastName##&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;&lt;strong&gt;Status: &lt;/strong&gt;##member.MemberStatus##&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            &lt;strong&gt;Enrollment Date:&lt;/strong&gt; ##AttrValue('MemberDetails', 'EnrollDate', 0)##
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            &lt;strong&gt;Last Activity Date:&lt;/strong&gt; ##member.LastActivityDate##
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            &lt;strong&gt;Tier:&lt;/strong&gt; ##GetCurrentTierProperty('DisplayText')##
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            &lt;strong&gt;Primary Email Address:&lt;/strong&gt; ##member.PrimaryEmailAddress##
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            &lt;strong&gt;Current Balance:&lt;/strong&gt; ##GetPoints('1/1/1900','12/31/2100')##
                        &lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/table&gt;</value>
  </data>
