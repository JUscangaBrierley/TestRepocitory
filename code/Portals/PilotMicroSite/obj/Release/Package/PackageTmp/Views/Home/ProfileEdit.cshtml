@using (Html.BeginForm("Profile", "Home", FormMethod.Post))
{
      @model PilotMicrosite.Models.Profile
            
             <div class="column_2">
    			<div class="column_2_container">
    				<div class="column_2_head">PROFILE</div>
            		<table width="100%" cellspacing="0" cellpadding="0" border="0">
            		<tr>
            		<td class="profile_table">
                		<table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_form_1">Name</td>
                		</tr>
                        <tr>
                		<td class="profile_info_form_2">@ViewBag.FirstName&nbsp;@ViewBag.LastName</td>
                		</tr>
                		</table>
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_form_1">Birthday</td>
                		</tr>
                        <tr>
                		<td class="profile_info_form_2">@ViewBag.Birthday&nbsp;</td>
                		</tr>
                		</table>
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_form_1">Gender</td>
                		</tr>
                        </table>
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_radio">@Html.RadioButtonFor(m => m.Gender, 1, new {id = "gender_male", Checked = @ViewBag.MaleChecked })<label for="gender_male">Male</label></td>
                        <td class="profile_info_radio">@Html.RadioButtonFor(m => m.Gender, 2, new {id = "gender_female", Checked = @ViewBag.FemaleChecked })<label for="gender_female">Female</label></td>
                		</tr>
                		</table>
                        <!--
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_radio">@Html.RadioButtonFor(m => m.Gender, "1", @ViewBag.Gender == "Male" ? new {Checked = "checked" }:null)<label for="gender_male">Male</label></td>
                        <td class="profile_info_radio">@Html.RadioButtonFor(m => m.Gender, "2", @ViewBag.Gender == "Female" ? new {Checked = "checked" }:null))<label for="gender_female">Female</label></td>
                		</tr>
                		</table>
                        -->
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_address">Address:</td>
                        </tr>
                        </table>
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_2">
                        	<span class="custom-dropdown custom-dropdown--white">
    							@Html.DropDownListFor(m => m.SelectedCountry,  new SelectList(@ViewBag.Countries, "Key", "Value", @ViewBag.SelectedCountry), new { @class = "custom-dropdown__select custom-dropdown__select--white" })
							</span></td>
                		</tr>
                		</table>
                         <div style="height:15px"></div>
                        
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_2">@Html.TextBoxFor(m => m.Address1, new { @Value = ViewBag.Address, @class = "form_input_inactive" })</td>
                		</tr>
                		</table>
                        
                        <div style="height:15px"></div>
                        
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_2">@Html.TextBoxFor(model => model.Address2, new { @Value = ViewBag.Address2, @class = "form_input_inactive" })</td>
                		</tr>
                		</table>
                        
                        <div style="height:15px"></div>
                        
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_form">@Html.TextBoxFor(model => model.City, new { @Value = ViewBag.City, @class = "form_input_inactive" })</td>
                        <td class="profile_info_spacer"></td>
                        <td class="profile_info_form"><span class="custom-dropdown custom-dropdown--white">
    							@Html.DropDownListFor(m => m.SelectedState,  new SelectList(@ViewBag.States, "Key", "Value", @ViewBag.SelectedState), new { Class ="custom-dropdown__select custom-dropdown__select--white"})
							</span></td>
                		</tr>
                		</table>
                        
                        
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="profile_info_address">Contact Info</td>
                        </tr>
                        </table>
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_2">@Html.TextBoxFor(model => model.EmailAddress, new { @Value = ViewBag.Email, @class = "form_input_inactive" })</td>
                		</tr>
                		</table>
                        
                          <div style="height:15px"></div>
                        
                         <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                        <td class="profile_info_2">@Html.TextBoxFor(model => model.MobilePhone, new { @Value = ViewBag.MobilePhone, @class = "form_input_inactive" })</td>
                		</tr>
                		</table>
                        
                          <div style="height:35px"></div>
                      
                        <table width="100%" cellspacing="0" cellpadding="0" border="0">
                		<tr>
                		<td class="button_cancel">@Html.ActionLink("Cancel", "Profile", "Cancel", new { @class = "link_cancel" })</td>
                        <td class="profile_info_spacer"></td>
                        <td class="button_update">@Html.ActionLink("Update", "Profile", "UpdateForm", new { @class = "link_update", onclick = "return false;" })</td>
                        
                        <!--<td class="button_update">@Html.ActionLink("Update", "Profile", "UpdateForm", new { @class = "link_update" })</td>-->
                        </tr>
                        </table></td>
            		</tr>
            		</table>
                    
                </div>
            </div>
    	<div style="clear:both"></div>
}
